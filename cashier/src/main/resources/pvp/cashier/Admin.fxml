<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pvp.cashier.controllers.AdminController">
   <children>
      <TableView fx:id="ProductView" prefHeight="270.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="productColumn" prefWidth="115.0" text="Product" />
            <TableColumn fx:id="priceColumn" prefWidth="79.0" text="Price" />
            <TableColumn fx:id="skuColumn" prefWidth="87.0" text="sku" />
            <TableColumn fx:id="pkColumn" prefWidth="126.0" text="pk" />
            <TableColumn fx:id="itemSoldColumn" prefWidth="65.0" text="Items sold" />
            <TableColumn fx:id="timeColumn" prefWidth="127.0" text="Time period" />
        </columns>
      </TableView>
      <TextField fx:id="timeInput" layoutX="23.0" layoutY="286.0" promptText="Time period" />
      <Button fx:id="periodLimitButton" layoutX="76.0" layoutY="338.0" mnemonicParsing="false" text="Limit" />
      <TextField fx:id="ageInput" layoutX="205.0" layoutY="286.0" promptText="Customer age" />
      <Button fx:id="ageLimitButton" layoutX="259.0" layoutY="338.0" mnemonicParsing="false" text="Limit" />
      <Button fx:id="sexLimitButtonM" layoutX="395.0" layoutY="286.0" mnemonicParsing="false" text="Male" />
      <Button fx:id="sexLimitButtonNone" layoutX="531.0" layoutY="286.0" mnemonicParsing="false" text="Both" />
      <Button fx:id="sexLimitButtonF" layoutX="459.0" layoutY="286.0" mnemonicParsing="false" text="Female" />
   </children>
</AnchorPane>
